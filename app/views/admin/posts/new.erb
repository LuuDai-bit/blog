<h1 class="admin-title">Create New Post</h1>

<div class="main-container">
  <%= form_with url: admin_posts_path,
                method: :post,
                scope: :post,
                data: { controller: 'emoji-picker' } do |form| %>
    <%= render partial: 'form', locals: { form: form, post: @post, action: 'create' } %>
  <% end %>
</div>
